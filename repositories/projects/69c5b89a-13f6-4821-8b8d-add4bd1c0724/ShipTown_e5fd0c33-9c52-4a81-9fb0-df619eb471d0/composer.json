{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "ext-simplexml": "*",
        "arrilot/laravel-widgets": "3.13.*",
        "aws/aws-sdk-php-laravel": "^3.10",
        "barryvdh/laravel-dompdf": "^3.1",
        "blackfire/php-sdk": "^1.31",
        "doctrine/dbal": "^3.9",
        "dompdf/dompdf": "^3.1",
        "guzzlehttp/guzzle": "^7.9",
        "hulkur/laravel-hasmany-keyby": "^7.0",
        "laravel/forge-sdk": "^3.21",
        "laravel/framework": "^11.44",
        "laravel/nightwatch": "^1.7",
        "laravel/passport": "^12.4",
        "laravel/telescope": "^5.5",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.6",
        "lcobucci/jwt": "^4.2",
        "league/csv": "^9.22",
        "league/flysystem-aws-s3-v3": "^3.29",
        "mehrancodes/laravel-harbor": "^1.1",
        "milon/barcode": "^11.0",
        "mtdowling/cron-expression": "*",
        "openai-php/laravel": "^0.11",
        "sentry/sentry-laravel": "^4.13",
        "spatie/array-to-xml": "^2.15",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-database-mail-templates": "^3.7",
        "spatie/laravel-permission": "^6.16",
        "spatie/laravel-query-builder": "^5.8",
        "spatie/laravel-ray": "^1.39",
        "spatie/laravel-tags": "^4.9",
        "thomasjohnkane/snooze": "^2.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.23",
        "jasonmccreary/laravel-test-assertions": "^2.6",
        "knuckleswtf/scribe": "^4.33",
        "laravel/dusk": "*",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "exclude-from-classmap": [
            "vendor/laravel-zero/foundation/src/Illuminate/",
            "vendor/mehrancodes/laravel-harbor/app/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "install-hooks": [
            "rm -rf .git/hooks",
            "ln -s ../scripts/hooks .git/hooks"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "pre-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@install-hooks"
        ],
        "post-install-cmd": [],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
